{
	"name": "notesIccee0",
	"displayName": "notesIccee0",
	"description": "",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:notes.helloWorld",
		"onCommand:notes.quickNote",
		"onCommand:notes.openDocumentation",
		"onCommand:fileExplorer.openFile",
		"onView:documentation",
		"onView:quicknote"
	],
	"publisher": "iccee0",
	"repository": {"url": "https://github.com/RoeHH/vscode-notes.git"},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "notes.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "notes.quickNote",
				"title": "Quick Note"
			},
			{
				"command": "notes.quickNote",
				"title": "qn"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "notes",
					"title": "Notes",
					"icon": "resources/notebook.svg"
				}
			]
		},
		"views": {
			"notes": [
				{
					"id": "documentation",
					"name": "Project Documentation",
					"contextualTitle": "Project Documentation"
				},
				{
					"id": "quicknote",
					"name": "QuickNote",
					"contextualTitle": "QuickNote"
				}
			]
		},
		"configuration": {
			"title": "Quick Notes",
			"properties": {
				"notes.noteDir": {
					"type": "string",
					"default": "C:\\quickNotes\\",
					"description": "Dir to store notes"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/ftp": "^0.3.33",
		"@types/glob": "^7.2.0",
		"@types/mkdirp": "^1.0.2",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/rimraf": "^3.0.2",
		"@types/uuid": "^8.3.4",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"ftp": "^0.3.10",
		"mkdirp": "^1.0.4",
		"uuid": "^8.3.2",
		"vsce": "^2.6.3"
	}
}
